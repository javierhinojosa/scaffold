{
  "name": "@sfh/backend",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./test-utils": {
      "types": "./dist/test-utils.d.ts",
      "import": "./dist/test-utils.mjs",
      "require": "./dist/test-utils.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/test-utils.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/test-utils.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "test": "vitest run --config ./vitest.config.ts",
    "test:watch": "vitest watch --config ./vitest.config.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.0.5",
    "@types/node": "^20.11.16",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
