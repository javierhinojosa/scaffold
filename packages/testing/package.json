{
  "name": "@sfh/testing",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth.js",
      "types": "./dist/auth.d.ts"
    },
    "./setup": {
      "import": "./dist/setup.js",
      "types": "./dist/setup.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/auth.ts src/setup.ts --format esm --dts",
    "dev": "tsup src/index.ts src/auth.ts src/setup.ts --format esm --dts --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.0.5",
    "vitest": "^1.2.2",
    "@supabase/supabase-js": "^2.39.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
